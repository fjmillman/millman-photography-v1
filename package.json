  "main": "public/index.php",
  "repository": "git@github.com:fjmillman/millmanphotography.git",
  "author": "fjmillman <freddie.millman@icloud.com>",
  "license": "Copyright 2017 Freddie John Millman",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.8.1",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "eslint": "^4.0.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "globstar": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cache": "^0.4.6",
    "gulp-cli": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-imagemin": "^3.3.0",
    "gulp-jshint": "^1.11.2",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-strip-debug": "^1.0.2",
    "gulp-stylus": "^2.6.0",
    "gulp-uglify": "^1.2.0",
    "jeet": "^7.1.0",
    "jshint-stylish": "^2.0.1",
    "lost": "^8.0.0",
    "nib": "^1.1.2",
    "onchange": "^3.2.1",
    "parallelshell": "^3.0.1",
    "postcss": "^6.0.3",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cli": "^3.2.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^2.0.6",
    "postcss-reporter": "^3.0.0",
    "postcss-smart-import": "^0.7.0",
    "postcss-url": "^6.0.4",
    "poststylus": "^0.2.3",
    "precss": "^1.4.0",
    "rimraf": "^2.6.1",
    "rupture": "^0.6.2",
    "stylus": "^0.52.0",
    "stylus-type-utils": "0.0.3",
    "webpack": "^3.0.0",
    "yargs": "^3.18.0"
  },
  "scripts": {
    "gulp": "gulp",
    "precss": "stylus -w assets/styles/*.styl -u jeet -u nib -u rupture -o assets/styles/millmanphotography.css",
    "precss:email": "stylus -w assets/styles/email/*.styl -u jeet -u nib -u rupture -o assets/styles/millmanphotography.css",
    "postcss": "postcss -c .postcssrc.json",
    "lint": "eslint assets/scripts/*.js",
    "uglify": "uglifyjs assets/scripts/*.js -m -o public/js/millmanphotography.js && uglifyjs public/js/millmanphotography.js -m -c -o public/js/millmanphotography.min.js",
    "imagemin": "imagemin assets/images public/img -p",
    "build:css": "yarn run precss && yarn run postcss",
    "build:email": "yarn run precss:email",
    "build:js": "yarn run lint && yarn run uglify",
    "build:images": "yarn run imagemin",
    "build:all": "yarn run build:css && yarn run build:js && yarn run build:images",
    "serve": "browser-sync start --proxy 'millmanphotography.dev' --files 'public/css/millmanphotography.min.css, public/js/millmanphotography.min.js'",
    "watch:css": "onchange 'assets/styles/*.styl' -- yarn run build:css",
    "watch:js": "onchange 'assets/scripts/*.js' -- yarn run build:js",
    "watch:all": "parallelshell 'yarn run serve' 'yarn run watch:css' 'yarn run watch:js'"
  },
  "dependencies": {
    "style-loader": "^0.18.2",
    "webpack-dev-server": "^2.5.0"
  }
}
